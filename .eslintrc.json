{
  "env": {
    "node": true,
    "es2020": true
  },
  "extends": [
    "airbnb-base",
    "plugin:@typescript-eslint/recommended",
    "prettier"
    // "plugin:prettier/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint",
    // "eslint-plugin-import-helpers",
    "prettier"
  ],
  "rules": {
  "prettier/prettier": "error",
  "no-console":"off",
  "no-underscore-dangle":"off"
  
  }
  
  }